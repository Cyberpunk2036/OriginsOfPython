class Matrix:
	def __init__(self, mtx):
		self.mtx = mtx
	def __str__(self):
		for i in self.mtx:
			for j in i:
				print(j, end=' ')
			print()
		return ""
	def __add__(self, other):
		for i in range(len(self.mtx)):
			for j in range(len(self.mtx[i])):
				print(int(self.mtx[i][j])+int(other.mtx[i][j]), end=' ')
			print()
		return ""
			
a = Matrix([[1, 2, 3],[4, 5, 6],[7, 8, 9]])
print(a)
b = Matrix([[9, 8, 7], [6, 5, 4], [3, 2, 1]])
print(a+b)
